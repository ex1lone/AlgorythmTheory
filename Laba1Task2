#include <stdio.h>

void printMas(int mas[], int n) {
    printf("\n");
    for (int i = 0; i < n; i++) {
        printf("%d ", mas[i]);
    }
}

void insertionSort(int mas[], int n) {
    for (int i = 1; i < n; i++) {

        int key = mas[i];
        int j = i - 1;

        while (j >= 0 && mas[j] > key) {
            mas[j + 1] = mas[j];
            j--;
        }

        mas[j + 1] = key;

        printMas(mas, n); // показати стан масиву на кожному кроці
    }
}

int main() {
    int mas[9] = {12, 23, 67, 65, 50, 70, 80, 61, 92};
    int n = 9;

    insertionSort(mas, n);

    return 0;
}
